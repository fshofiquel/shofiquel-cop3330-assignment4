@startuml
'https://plantuml.com/class-diagram

class ToDoList
{
    main()
    start()
}

class ToDoListDriver
{
    - toDoListName: TextField
    - editTasks: TextField
    - toDoList: ListView
    - tasksList: ListView


    + addToDoClick()
    + importClick()
    + exportClick()
    + addOrEditClick()
    + viewClick()
}


javafx.Application <|-- ToDoList

ToDoList -- ToDoListDriver



@enduml